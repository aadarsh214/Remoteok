{
  "name": "remoteok",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "deploy:cf": "npm run -w web build && npx wrangler pages deploy apps/web/.vercel/output/static --functions apps/web/.vercel/output/functions"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "prisma": "^7.0.0",
    "turbo": "^2.6.0",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "npm@11.6.2",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
